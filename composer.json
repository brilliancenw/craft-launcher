{
    "name": "brilliance/craft-launcher",
    "description": "Universal search launcher for Craft CMS admin panel",
    "type": "craft-plugin",
    "version": "1.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "launcher",
        "search",
        "admin"
    ],
    "support": {
        "docs": "https://github.com/brilliance/craft-launcher/blob/main/README.md",
        "issues": "https://github.com/brilliance/craft-launcher/issues"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Brilliance",
            "homepage": "https://www.brilliancenw.com/"
        }
    ],
    "require": {
        "php": "^8.2",
        "craftcms/cms": "^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "brilliance\\launcher\\": "src/"
        }
    },
    "extra": {
        "name": "Launcher",
        "handle": "launcher",
        "class": "brilliance\\launcher\\Launcher"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "yiisoft/yii2-dev": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
